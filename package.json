{
  "name": "advent-2021",
  "version": "1.0.0",
  "description": "Help Santa Claus",
  "main": "./lib/index.js",
  "bin": {
    "advent": "./lib/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "local": "sudo npm i -g && advent",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "test": "jest",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hatunike/advent-2021.git"
  },
  "keywords": [
    "advent",
    "typescript",
    "2021"
  ],
  "author": "Charles",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hatunike/advent-2021/issues"
  },
  "homepage": "https://github.com/hatunike/advent-2021#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^8.3.0",
    "figlet": "^1.5.2",
    "inquirer": "^8.2.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/clear": "^0.1.2",
    "@types/commander": "^2.12.2",
    "@types/figlet": "^1.5.4",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
